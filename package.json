{
	"name": "create-rapo",
	"version": "0.0.11",
	"description": "A CLI tool to quickly scaffold React projects with pre-configured templates",
	"type": "module",
	"main": "dist/index.js",
	"bin": {
		"create-rapo": "dist/index.js"
	},
	"files": [
		"dist",
		"templates",
		"!templates/*/node_modules",
		"!templates/*/dist",
		"!templates/*/pnpm-lock.yaml",
		"README.md"
	],
	"keywords": [
		"create-rapo",
		"cli",
		"scaffold",
		"template",
		"boilerplate",
		"generator"
	],
	"author": "Ogabek Yuldoshev",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "https://github.com/OgabekYuldoshev/create-rapo.git"
	},
	"bugs": {
		"url": "https://github.com/OgabekYuldoshev/create-rapo/issues"
	},
	"homepage": "https://github.com/OgabekYuldoshev/create-rapo#readme",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"release": "standard-version",
		"dev": "tsx src/index.ts",
		"build": "tsup src/index.ts",
		"lint": "biome check --write .",
		"prepublishOnly": "pnpm release && pnpm build"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"dependencies": {
		"@clack/prompts": "^0.11.0",
		"chalk": "^5.6.2",
		"minimist": "^1.2.8"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.10",
		"@types/minimist": "^1.2.5",
		"@types/node": "^25.0.3",
		"standard-version": "^9.5.0",
		"tsup": "^8.5.1",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3"
	},
	"packageManager": "pnpm@10.25.0",
	"volta": {
		"node": "24.12.0"
	}
}
